{
  "mcpServers": {
    "github": {
      "httpUrl": "https://api.githubcopilot.com/mcp/",
      "headers": {
        "Authorization": "Bearer <TOKEN GITHUB>"
      },
      "timeout": 5000
    },
    "context7": {
      "httpUrl": "https://mcp.context7.com/mcp",
      "headers": {
        "Authorization": "Bearer <API CONTEXT7>",
        "Accept": "application/json, text/event-stream"
      }
    }
  }
}
